  <dependencies>
    <dependency>
      <groupId>net.tiny</groupId>
      <artifactId>tiny-boot</artifactId>
      <version>1.0.0</version>
    </dependency>
    <dependency>
      <groupId>net.tiny</groupId>
      <artifactId>tiny-service</artifactId>
